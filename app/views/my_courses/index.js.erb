<% if @courses.present? %>
  $("#courses").html("<%= j render collection: @courses, partial: "my_courses/course" %>");
<% else %>
  $("#courses").html("<p><%= j t("common.no_course_found") %></p>");
<% end %>
$("#courses-paginate").html("<%= j paginate @courses, remote: true, theme: "twitter-bootstrap-3" %>");
