<tr>
  <td><%= page_index params[:page], user_counter, Settings.per %></td>
  <td><%= user.code %></td>
  <td><%= user.email %></td>
  <td><%= user.role %></td>
  <td>
    <div class="inline-item">
      <%= link_to edit_user_path(user), {class: "btn btn-primary"} do %>
        <i class="fas fa-pen"></i>
      <% end %>
    </div>
  </td>
</tr>
